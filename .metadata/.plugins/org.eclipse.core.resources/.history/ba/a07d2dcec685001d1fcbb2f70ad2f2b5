<mapper namespace="notice">
	<select id="noticeList" resultType="kr.go.mofa.dto.SampleDTO">
		select * from notice
	</select>
	<select id="noticeOne" resultType="kr.go.mofa.dto.SampleDTO">
		select * from notice where id = #{id}
	</select>
	<insert id="addNotice">
		insert into notice(title, content, dept, regdate) values #{title}, #{content}, #{dept}, now()

	</insert>
</mapper>